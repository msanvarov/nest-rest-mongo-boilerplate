{
  "name": "nest-mongoose-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "msanvarov",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "typedocs": "npx compodoc -p tsconfig.json -s -d docs",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "7.0.13",
    "@nestjs/core": "7.0.13",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/mongoose": "7.0.0",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.13",
    "@nestjs/platform-fastify": "7.0.13",
    "@nestjs/swagger": "4.5.6",
    "@nestjs/typeorm": "7.0.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "crypto": "1.0.1",
    "dotenv": "8.2.0",
    "express-rate-limit": "5.1.3",
    "fastify-helmet": "3.0.2",
    "fastify-rate-limit": "3.0.1",
    "fastify-swagger": "2.5.1",
    "gravatar": "1.8.0",
    "helmet": "3.22.0",
    "mongoose": "5.9.14",
    "nest-access-control": "2.0.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "swagger-ui-express": "4.1.4",
    "typeorm": "0.2.24",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@nestjs/testing": "7.0.13",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.6",
    "@types/gravatar": "1.8.1",
    "@types/hapi__joi": "17.1.0",
    "@types/jest": "25.2.2",
    "@types/mongoose": "5.7.19",
    "@types/node": "13.13.5",
    "@types/supertest": "2.0.9",
    "chai": "4.2.0",
    "jest": "26.0.1",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "25.5.1",
    "ts-loader": "7.0.4",
    "ts-node": "8.10.1",
    "tsc-watch": "4.2.5",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "typescript": "3.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
