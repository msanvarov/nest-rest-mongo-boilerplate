{
  "name": "nest-mongoose-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "msanvarov",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "typedocs": "npx compodoc -p tsconfig.json -s -d docs",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@hapi/joi": "17.0.0",
    "@nestjs/common": "6.10.14",
    "@nestjs/core": "6.10.14",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/mongoose": "6.1.2",
    "@nestjs/passport": "6.1.1",
    "@nestjs/platform-express": "6.10.14",
    "@nestjs/platform-fastify": "6.10.14",
    "@nestjs/swagger": "3.1.0",
    "@nestjs/typeorm": "6.2.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.0",
    "crypto": "1.0.1",
    "dotenv": "8.2.0",
    "express-rate-limit": "5.0.0",
    "fastify-helmet": "3.0.2",
    "fastify-rate-limit": "3.0.1",
    "fastify-swagger": "2.5.0",
    "gravatar": "1.8.0",
    "helmet": "3.21.2",
    "mongoose": "5.8.6",
    "nest-access-control": "2.0.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.4",
    "swagger-ui-express": "4.1.2",
    "typeorm": "0.2.22",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@nestjs/testing": "6.10.14",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.2",
    "@types/gravatar": "1.8.0",
    "@types/hapi__joi": "16.0.6",
    "@types/jest": "24.0.25",
    "@types/mongoose": "5.5.40",
    "@types/node": "12.12.21",
    "@types/supertest": "2.0.8",
    "chai": "4.2.0",
    "jest": "24.9.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "tsc-watch": "4.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
